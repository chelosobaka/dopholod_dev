<%= form_with(model: cell, local: true) do |form| %>
  <% if cell.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(cell.errors.count, "error") %> prohibited this cell from being saved:</h2>

      <ul>
      <% cell.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :temperature_range %>
    <%= form.text_field :temperature_range %>
  </div>

  <div class="field">
    <%= form.label :space %>
    <%= form.text_field :space %>
  </div>

  <div class="field">
    <%= form.label :dimensions %>
    <%= form.text_field :dimensions %>
  </div>

  <div class="field">
    <%= form.label :net_weight %>
    <%= form.number_field :net_weight %>
  </div>

  <div class="field">
    <%= form.label :packed_sizes %>
    <%= form.text_field :packed_sizes %>
  </div>

  <div class="field">
    <%= form.label :product_id_id %>
    <%= form.text_field :product_id_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
